<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="49.0" prefWidth="254.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jabbermessenger.controllers.FollowUserController">
    <children>
        <Label fx:id="userToFollow" prefHeight="29.0" prefWidth="85.0">
            <font>
                <Font name="Arial" size="13.0" />
            </font></Label>
        <Pane prefHeight="49.0" prefWidth="65.0" />
        <Button fx:id="followButton" mnemonicParsing="false" onAction="#followUser" prefHeight="53.0" prefWidth="63.0" text="+" textFill="#0b0d0a">
            <font>
                <Font name="System Bold" size="20.0" />
            </font></Button>
    </children>
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
</HBox>
