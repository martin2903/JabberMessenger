<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="64.0" prefWidth="650.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jabbermessenger.controllers.TimeLineController">
    <children>
        <Label fx:id="userName" prefHeight="61.0" prefWidth="118.0" text="DonaldJTrump">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font>
        </Label>
        <Pane prefHeight="64.0" prefWidth="35.0" />
        <Label fx:id="jab" alignment="TOP_LEFT" prefHeight="61.0" prefWidth="475.0" wrapText="true">
            <font>
                <Font name="Arial" size="12.0" />
            </font></Label>
        <Button fx:id="timeLineBtn" maxHeight="20.0" maxWidth="20.0" mnemonicParsing="false" onAction="#addLike" prefHeight="20.0" prefWidth="20.0">
            <graphic>
                <ImageView fx:id="likeButton" fitHeight="24.0" fitWidth="21.0" pickOnBounds="true" preserveRatio="true" />
            </graphic>
        </Button>
        <Pane prefHeight="64.0" prefWidth="63.0">
            <children>
                <Label fx:id="numLikes" layoutX="18.0" layoutY="-8.0" prefHeight="17.0" prefWidth="50.0" text="20" />
            </children></Pane>
    </children>
    <padding>
        <Insets top="20.0" />
    </padding>
</HBox>
